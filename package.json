{
    "name": "process-manager",
    "version": "2.0.0",
    "private": true,
    "scripts": {
        "build": "yarn less && next build && next export",
        "build:analyze": "cross-env ANALYZE=true next build",
        "clean": "rimraf ./out",
        "dev": "yarn less && next dev",
        "less": "lessc --js styles/openfin-antd.less styles/openfin-antd.css",
        "serve": "npx http-server ./out -p 3000",
        "start": "yarn build && yarn serve"
    },
    "dependencies": {
        "@ant-design/colors": "^5.0.1",
        "@ant-design/icons": "^4.4.0",
        "antd": "^4.12.2",
        "copy-webpack-plugin": "5.1.1",
        "less": "^4.1.1",
        "next": "10.0.2",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-resizable": "1.11.0"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^10.0.3",
        "@types/node": "14.14.9",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "cross-env": "^7.0.3",
        "openfin-adapter": "^18.57.25",
        "rimraf": "^3.0.2",
        "typescript": "4.1.2"
    }
}
